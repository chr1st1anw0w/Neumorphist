{
  "name": "Neumorphism Designer",
  "id": "neumorphism-designer-plugin",
  "api": "1.0.0",
  "main": "code.js",
  "ui": "ui.html",
  "icon": "icon.svg",
  "editorType": [
    "figma"
  ],
  "parameters": [
    {
      "name": "Open Neumorphism Designer",
